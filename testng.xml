<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Cooee Web Automation Suite" parallel="false">

  <test name="Chrome Test">
    <parameter name="browser" value="chrome"/>
    <classes>
      <class name="org.cooee.tests.LoginTest"/>
    </classes>
  </test>

  <test name="Edge Test">
    <parameter name="browser" value="edge"/>
    <classes>
      <class name="org.cooee.tests.LoginTest"/>
    </classes>
  </test>

</suite>
